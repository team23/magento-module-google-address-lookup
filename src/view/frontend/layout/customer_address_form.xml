<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block ifconfig="google_address_lookup/general/active"
                   class="Magento\Framework\View\Element\Template"
                   name="google.address.lookup.customer.address"
                   template="Team23_GoogleAddressLookup::customer/google-address-lookup.phtml">
                <arguments>
                    <argument name="view_model"
                              xsi:type="object">Team23\GoogleAddressLookup\ViewModel\GoogleAddressLookup</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
